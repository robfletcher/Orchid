<!DOCTYPE html>
<html lang="en">
<head>
{% include page|templates('trackingHeadStart', true) %}
{% include page|templates('includes/head') %}
{% include page|templates('includes/styles') %}
{% block layoutStyles %}{% endblock %}
{% include page|templates('trackingHeadEnd', true) %}
</head>
<body>
{% include page|templates('trackingBodyStart', true) %}

{% block baseNavbar %}
{% if theme.social.github is not empty %}
<a class="github-fork-ribbon {% block ribbonSide %}{{ 'left-top' }}{% endblock %}"
   href="https://github.com/{{ theme.social.github }}"
   title="Fork me on GitHub">Fork me on GitHub</a>
{% endif %}

{% include page|templates('includes/navbar') %}
{% endblock %}

<div id="top"></div>

{% block baseJumbotron %}
{% endblock %}
{% block baseBody %}
<div class="container bs-docs-container">
    <div class="row">
        <div class="col-md-9" role="main">
        {% block pageContent %}

        {% endblock %}
        {% block remainingComponents %}
            {% include page|templates('includes/componentHolder') with {"componentHolder": page.components} %}
        {% endblock %}
        </div>

        <!-- Page navigation
        =========================================== -->
        <div class="col-md-3" role="complementary">
        {% include page|templates('includes/sidenav') %}
        </div>
    </div>
</div>
{% endblock %}
{% block baseFooter %}
<div id="footer">
    <div class="container bs-docs-container">
    {% include page|templates('includes/footer') %}
    </div>
</div>
{% endblock %}

{% include page|templates('includes/scripts') %}
{% block layoutScripts %}{% endblock %}
<script>
    $(function () {
        window.colors = ['{{ theme.primaryColor }}', '{{ theme.secondaryColor }}'];
                  {% block setupJumbotron %}{% endblock %}
        buildNav();
    });
</script>
{% include page|templates('trackingBodyEnd', true) %}
</body>
</html>